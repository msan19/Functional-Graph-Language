export Cycle(5) {"Cycle"}

Cycle: (integer) -> graph
Cycle(n) = (elements(n), elements(n), src, (element e) => dst(e,n))

elements:(integer) -> set
elements(n) = {e[i] | 0<=[i]<n}

src: (element) -> element
src(e) =
	| e[i] = e

dst: (element, integer) -> element
dst(e, n) =
	| e[i] = element[(i+1) mod n]